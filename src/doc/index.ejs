<%
    var url = locals.url || '';
    var cdnUrl =  url + 'dist/src/';
%>
<!DOCTYPE html>
<html lang="<%=locals.lang||'ch'%>">

<head>
    <script>
    var host = "shishiv30.github.io";
    if ((host == window.location.host) && (window.location.protocol != "https:"))
        window.location.protocol = "https";
    </script>
    <title><%= __('CUI framewrok') %></title>
    <meta http-equiv="Cache-control" content="public">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta content='width=device-width, initial-scale=1.0' name='viewport'/>
    <meta description='the documation of UI framework' />
    <meta name="Description" content="Author: Conjee Zou, UI framework, Category: Document, Lisence: MIT">
    <meta name="background-color" content="#ffffff">
    <meta name="theme-color" content="#333333">
    <link rel="manifest" href="<%= cdnUrl +'doc/src/manifest.json' %>">
    <link href="<%=cdnUrl + 'cui.min.css'%>" type="text/css" rel="stylesheet" />
    <link rel="shortcut icon" href="<%=cdnUrl +'img/favicon.ico'%>" type="image/x-icon">
    <link rel="icon" href="<%=cdnUrl +'favicon.ico'%>" type="image/x-icon">
</head>
<%- include('./views/tpl-header') %>
<body id="body" class="classic">

<div class="gs-fixed">
    <%- include('./views/tpl') %>
    <%- include('./views/plugin') %>
</div>
</body>
<script>
    window.context = {
        url: window.location.origin,
        cdnUrl:window.location.origin + '/dist/src/',
        googleMapKey:'key=AIzaSyBtspK44Jc-DabkyA6knFuQ6LSp_8VXZgc'
    };
    if ('serviceWorker' in navigator) {
       navigator.serviceWorker.register('./sw.js');
    }
</script>
<script src="<%= cdnUrl +'cui.lib.min.js' %>" type="text/javascript"></script>
<script src="<%= cdnUrl +'cui.min.js' %>" type="text/javascript"></script>

<script>
    $.preload({
        files:['dist/src/doc/src/doc.css','dist/src/doc/src/doc.js']
    });
</script>
</html>
