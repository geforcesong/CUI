<%
    var url =  rootUrl;
    var cdnUrl =  url + 'dist/src/';
%>
<!DOCTYPE html>
<html>
<head>
    <title></title>
    <link href="<%=cdnUrl + 'cui.min.css'%>" type="text/css" rel="stylesheet" />
    <link href="<%=cdnUrl + 'demo1/src/demo1.css'%>" type="text/css" rel="stylesheet" />
</head>
<style>
body{
    background:#fff;
}
.slicker{
    height:50vw;
}
.logo {
    display:flex;
    align-items: center;
}
.logo img{
    height:80px;
}
.logo a{
    flex-shrink: 0;
}
.navigator{
    position: sticky;
    z-index: 999999;
    top: 0;
    display:flex;
    height:70px;
    overflow:hidden;
    flex-wrap: wrap;
    justify-content: space-between;
    text-align:center;
    align-items: center;
    box-shadow:0 0 1px 1px #eee;
    background: darkgreen;
    color: #fff;
}
.navigator > a {
    display:block;
    padding:10px;
    color:inherit;
    opacity:0.7;
}
.navigator > a:hover {
    opacity:1;
}
.content-panel{
    text-align:center;
    display:flex;
    min-height:20vw;
    align-items: center;
    justify-content: center;
    padding:5px 30px;
}
.content-list li{
    line-height: 2em;
    text-align:left;
}
.content-list li.link{
    color:#4facfe;
    text-align:right;
}
.dropdown-button{
    margin:5px 0;
}
.footer{
    text-align:center;
    line-height:2em;
}
</style>
<body id="body" class="classic">
    <div class=" logo">
        <div>
            <a href="#" class="navigator-logo">
                <img src="dist/src/demo1/src/logo.jpg" alt="logo">
            </a>
        </div>
        <div>
            Dream Chaser—Chase Dream With You, Together!
        </div>
    </div>
    <div class="navigator">
        <a href="#">
            <p>WHY NZ</p>
            <p>选择新西兰</p>
        </a>
        <a href="#">
            <p>STUDY TOUR</p>
            <p>游学新西兰</p>
        </a>
        <a href="#">
            <p>STUDY OPTION</p>
            <p>留学新西兰</p>
        </a>
        <a href="#">
            <p>LIVING IN NZ</p>
            <p>探索新西兰</p>
        </a>
        <a href="#">
            <p>CONTACT US</p>
            <p>联系我们</p>
        </a>
